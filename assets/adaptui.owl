                        </rdf:first>
                        <rdf:rest>
                          <swrl:AtomList>
                            <rdf:first>
                              <swrl:BuiltinAtom>
                                <swrl:arguments>
                                  <rdf:List>
                                    <rdf:first rdf:resource="http://www.morelab.deusto.es/ontologies/value"/>
                                    <rdf:rest>
                                      <rdf:List>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                        <rdf:first rdf:datatype=
                                        "http://www.w3.org/2001/XMLSchema#int"
                                        >50</rdf:first>
                                      </rdf:List>
                                    </rdf:rest>
                                  </rdf:List>
                                </swrl:arguments>
                                <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                              </swrl:BuiltinAtom>
                            </rdf:first>
                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                          </swrl:AtomList>
                        </rdf:rest>
                      </swrl:AtomList>
