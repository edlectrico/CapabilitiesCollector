                    <swrl:argument1 rdf:resource="http://www.morelab.deusto.es/ontologies/caux"/>
                    <swrl:classPredicate rdf:resource="#ContextAux"/>
                  </swrl:ClassAtom>
                </rdf:first>
              </swrl:AtomList>
            </rdf:rest>
            <rdf:first>
              <swrl:ClassAtom>
                <swrl:classPredicate rdf:resource="http://u2m.org/2003/02/UserModelOntology.rdf#Noise"/>
                <swrl:argument1 rdf:resource="http://www.morelab.deusto.es/ontologies/noise"/>
