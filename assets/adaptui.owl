  <swrl:Imp rdf:about="http://www.morelab.deusto.es/ontologies/checkNoiseJackhammer">
    <swrl:body>
      <swrl:AtomList>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:rest>
              <swrl:AtomList>
                <rdf:first>
                  <swrl:ClassAtom>
                    <swrl:argument1>
                      <swrl:Variable rdf:about="http://www.morelab.deusto.es/ontologies/caux"/>
                    </swrl:argument1>
                    <swrl:classPredicate rdf:resource="#ContextAux"/>
                  </swrl:ClassAtom>
                </rdf:first>
                <rdf:rest>
                  <swrl:AtomList>
                    <rdf:rest>
                      <swrl:AtomList>
                        <rdf:first>
                          <swrl:BuiltinAtom>
                            <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThanOrEqual"/>
                            <swrl:arguments>
                              <rdf:List>
                                <rdf:rest>
                                  <rdf:List>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                                    >130</rdf:first>
                                  </rdf:List>
                                </rdf:rest>
                                <rdf:first>
                                  <swrl:Variable rdf:about="http://www.morelab.deusto.es/ontologies/value"/>
                                </rdf:first>
                              </rdf:List>
                            </swrl:arguments>
                          </swrl:BuiltinAtom>
                        </rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </swrl:AtomList>
                    </rdf:rest>
                    <rdf:first>
                      <swrl:DatavaluedPropertyAtom>
                        <swrl:argument2 rdf:resource="http://www.morelab.deusto.es/ontologies/value"/>
                        <swrl:argument1>
                          <swrl:Variable rdf:about="http://www.morelab.deusto.es/ontologies/noise"/>
                        </swrl:argument1>
                        <swrl:propertyPredicate rdf:resource="#contextHasNoise"/>
                      </swrl:DatavaluedPropertyAtom>
                    </rdf:first>
                  </swrl:AtomList>
                </rdf:rest>
              </swrl:AtomList>
            </rdf:rest>
            <rdf:first>
              <swrl:ClassAtom>
                <swrl:argument1>
                  <swrl:Variable rdf:about="http://www.morelab.deusto.es/ontologies/context"/>
                </swrl:argument1>
                <swrl:classPredicate rdf:resource="#Context"/>
              </swrl:ClassAtom>
            </rdf:first>
          </swrl:AtomList>
        </rdf:rest>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:argument1 rdf:resource="http://www.morelab.deusto.es/ontologies/noise"/>
            <swrl:classPredicate rdf:resource="http://u2m.org/2003/02/UserModelOntology.rdf#Noise"/>
          </swrl:ClassAtom>
        </rdf:first>
      </swrl:AtomList>
    </swrl:body>
    <swrl:head>
      <swrl:AtomList>
        <rdf:first>
          <swrl:DatavaluedPropertyAtom>
            <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >jackhammer</swrl:argument2>
            <swrl:argument1 rdf:resource="http://www.morelab.deusto.es/ontologies/caux"/>
            <swrl:propertyPredicate rdf:resource="#contextAuxHasNoiseLevel"/>
          </swrl:DatavaluedPropertyAtom>
        </rdf:first>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
      </swrl:AtomList>
    </swrl:head>
  </swrl:Imp>
