    <!-- http://www.morelab.deusto.es/ontologies/adaptui#contextLightAffects -->

    <owl:ObjectProperty rdf:about="http://www.morelab.deusto.es/ontologies/adaptui#contextLightAffects">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;p style=&quot;margin-top: 0&quot;&gt;
      Context light affects several capabilities:
    &lt;/p&gt;
    &lt;ul&gt;
      &lt;li&gt;
        User: visual capabilities (Display, Interface)
      &lt;/li&gt;
      &lt;li&gt;
        Device: visual based interaction (Contrast, Brightness) &amp;#160;
      &lt;/li&gt;
    &lt;/ul&gt;</rdfs:comment>
        <rdfs:domain rdf:resource="http://u2m.org/2003/02/UserModelOntology.rdf#Light"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.morelab.deusto.es/ontologies/adaptui#Brightness"/>
                    <rdf:Description rdf:about="http://www.morelab.deusto.es/ontologies/adaptui#Contrast"/>
                    <rdf:Description rdf:about="http://www.morelab.deusto.es/ontologies/adaptui#Display"/>
                    <rdf:Description rdf:about="http://www.morelab.deusto.es/ontologies/adaptui#Interface"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


